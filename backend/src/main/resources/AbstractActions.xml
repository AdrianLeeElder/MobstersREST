<?xml version="1.0" encoding="utf-8" ?>
<AbstractAction>
    <action>
        <name>Bank</name>
        <finishText>Currently in your bank account</finishText>
        <xPath><xPath>//span[contains(text(), 'Bank')]</xPath></xPath>
    </action>
    <action>
        <name>200 Energy Link</name>
        <finishText>Neeko has graciously rewarded you with an EXTRA 200 energy boost!</finishText>
        <finishText>You have already collected your 200 energy for today. Please come back tomorrow.</finishText>
        <xPath>//*[@id="content"]/div[5]/div/h3/a[1]</xPath>
        <fancyBox>true</fancyBox>
    </action>
    <action>
        <name>Daily Bonus Link</name>
        <finishText>Neeko has graciously rewarded you with</finishText>
        <finishText>collected your daily bonus</finishText>
        <xPath>//*[@id="content"]/div[5]/div/h3/a[2]</xPath>
        <fancyBox>true</fancyBox>
    </action>
    <action>
        <name>My Mob</name>
        <finishText>Active Top Mob Slots</finishText>
        <xPath>//span[contains(text(), 'My Mob ')]</xPath>
    </action>
    <action>
        <name>Accept All</name>
        <finishText>You have successfully added</finishText>
    </action>
    <action>
        <name>Optimizer Popup</name>
        <finishText>This will replace your current Top Mob</finishText>
        <xPath>//a[text()='Optimizer']</xPath>
    </action>
    <action>
        <name>Optimizer</name>
        <finishText>Successo!</finishText>
        <xPath>//*[@id="fancybox-content"]/div/button[1]</xPath>
    </action>
    <action>
        <name>Save Current Popup</name>
        <finishText>This will save your current Top Mob to be</finishText>
    </action>
    <action>
        <name>Save Current</name>
        <finishText>Top Mob Saved</finishText>
        <xPath>//*[@id="tmob"]/div/div/div[1]/span/span/div[2]/a</xPath>
    </action>
    <action>
        <name>Send All</name>
        <finishText>You have sent a</finishText>
        <xPath>//*[@id="tmob"]/div/div/div[1]/span/span/div[5]/a</xPath>
    </action>
    <action>
        <name>Collect All</name>
        <finishText>You have collected</finishText>
        <xPath>//a[text()='Collect All']</xPath>
    </action>
    <action>
        <name>Restore Popup</name>
        <finishText>This will demote everyone in your current top mob and restore</finishText>
        <xPath>//a[text()='Restore to Saved']</xPath>
    </action>
    <action>
        <name>Restore</name>
        <finishText>Top Mob Restored</finishText>
        <xpath>//*[@id="fancybox-content"]/div/button[1]</xpath>
    </action>
    <action>
        <name>Deposit</name>
        <finishText>You have successfully deposited</finishText>
        <xPath>//*[@id="bank"]/div[2]/button</xPath>
    </action>
    <action>
        <name>Territory</name>
        <finishText>newsstand</finishText>
        <xPath>//span[contains(text(), 'Territory')]</xPath>
    </action>
    <action>
        <name>Withdraw</name>
        <finishText>fancybox-tmp</finishText>
        <xPath>//*[@id="bank"]/div[3]/button</xPath>
    </action>
    <action>
        <name>Withdraw Popup</name>
        <finishText>You have successfully withdrawn</finishText>
        <xPath>//*[@id="fancybox-content"]/div/button[1]</xPath>
    </action>
    <action>
        <name>Logout</name>
        <finishText>WELCOME TO MOBSTERS PLAYERS REVENGE!</finishText>
        <xPath>//*[@id="headerFrame"]/center/nav/div/div[4]/a</xPath>
    </action>
    <action>
        <name>Login</name>
        <finishText>Welcome to MPR</finishText>
    </action>
    <action>
        <name>My Mobster</name>
        <finishText>Convene with your fellow mobbie</finishText>
        <xPath>//span[contains(text(), 'My Mob')]</xPath>
    </action>
    <action>
        <name>Refresh</name>
        <finishText></finishText>
        <xPath>//div[text()='REFRESH']</xPath>
    </action>
    <action>
        <name>Fancy Box</name>
        <xPath>//*[@id="fancybox-close"]</xPath>
    </action>
</AbstractAction>